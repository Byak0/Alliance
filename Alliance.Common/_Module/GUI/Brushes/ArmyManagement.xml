  <Brush Name="ArmyManagement.Army.Tuple">
    <Layers>
      <BrushLayer Name="Default" Sprite="ArmyManagement\voter_tuple" />
      <BrushLayer Name="Selected" Sprite="ArmyManagement\voter_tuple_selected" />
    </Layers>
    <Styles>
      <Style Name="Default">
        <BrushLayer Name="Default" />
        <BrushLayer Name="Selected" IsHidden="true" />
      </Style>
      <Style Name="Hovered">
        <BrushLayer Name="Default" ColorFactor="1.3" />
        <BrushLayer Name="Selected" IsHidden="true" />
      </Style>
      <Style Name="Selected">
        <BrushLayer Name="Default" IsHidden="true" />
        <BrushLayer Name="Selected" />
      </Style>
      <Style Name="Disabled">
        <BrushLayer Name="Default" ColorFactor="0.4" />
