    <Layers>
      <BrushLayer Name="Default" Sprite="CharacterDeveloper\character_perk_background" />
    </Layers>
    <Styles>
      <Style Name="Default" AnimationToPlayOnBegin="DefaultAnimation">
        <BrushLayer Name="Default" ColorFactor="1.0" />
      </Style>
      <Style Name="Pressed">
        <BrushLayer Name="Default" ColorFactor="0.8" />
      </Style>
      <Style Name="Hovered" AnimationToPlayOnBegin="HoveredAnimation">
        <BrushLayer Name="Default" ColorFactor="1.2" />
      </Style>
      <Style Name="Selected">
        <BrushLayer Name="Default" ColorFactor="1.0" />
      </Style>
    </Styles>
    <Animations>
      <Animation Name="DefaultAnimation" Duration="1" Loop="true">
        <AnimationProperty LayerName="Default" PropertyName="ColorFactor">
          <KeyFrame Time="0" Value="0.8" />
          <KeyFrame Time="0.5" Value="2.5" />
          <KeyFrame Time="1" Value="0.8"/>
        </AnimationProperty>
      </Animation>
      <Animation Name="HoveredAnimation" Duration="0.5" Loop="true">
        <AnimationProperty LayerName="Default" PropertyName="ColorFactor">
          <KeyFrame Time="0" Value="2" />
          <KeyFrame Time="0.25" Value="2.5" />
          <KeyFrame Time="0.5" Value="2" />
        </AnimationProperty>
      </Animation>
    </Animations>
  </Brush>

