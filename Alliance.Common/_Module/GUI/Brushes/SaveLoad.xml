<Brushes>
  <Brush Name="SaveLoad.Tuple">
    <Layers>
      <BrushLayer Name="Default" Sprite="SaveLoad\load_tuple" />
      <BrushLayer Name="Selected" Sprite="SaveLoad\load_tuple_selected" />
    </Layers>
    <Styles>
      <Style Name="Default">
        <BrushLayer Name="Default" ColorFactor="1" />
        <BrushLayer Name="Selected" IsHidden="true" />
      </Style>
      <Style Name="Hovered">
        <BrushLayer Name="Default" ColorFactor="1.2" />
        <BrushLayer Name="Selected" IsHidden="true" />
      </Style>
      <Style Name="Pressed">
        <BrushLayer Name="Default" ColorFactor="0.8" />
        <BrushLayer Name="Selected" IsHidden="true" />
      </Style>
      <Style Name="Selected">
        <BrushLayer Name="Default" IsHidden="true" />
        <BrushLayer Name="Selected" />
      </Style>
      <Style Name="Disabled">
        <BrushLayer Name="Default" AlphaFactor="0.6" SaturationFactor="-80" ValueFactor="-20" />
        <BrushLayer Name="Selected" IsHidden="true" />
      </Style>
    </Styles>
  </Brush>
  <Brush Name="SaveLoad.Search.Button">
    <Layers>
      <BrushLayer Name="Default" Sprite="StdAssets\tabbar_long_namebox" />
    </Layers>
    <Styles>
      <Style Name="Default">
        <BrushLayer Name="Default" ColorFactor="1.1" />
      </Style>
      <Style Name="Hovered">
        <BrushLayer Name="Default" ColorFactor="1.4" />
      </Style>
      <Style Name="Pressed">
        <BrushLayer Name="Default" ColorFactor="0.75" />
      </Style>
      <Style Name="Disabled">
        <BrushLayer Name="Default" ColorFactor="0.4" />
      </Style>
    </Styles>
  </Brush>
  <Brush Name="SaveLoad.Search.InputText" Font="FiraSansExtraCondensed-Regular" TextHorizontalAlignment="Left" TextVerticalAlignment="Center">
    <Styles>
      <Style Name="Default" FontColor="#F9EAD2FF" TextGlowColor="#000000FF" TextOutlineColor="#000000FF" TextOutlineAmount="0" TextGlowRadius="0" TextBlur="0" FontSize="28" />
    </Styles>
  </Brush>
  <Brush Name="SaveLoad.Search.PlaceholderText" Font="FiraSansExtraCondensed-Light" TextHorizontalAlignment="Left" TextVerticalAlignment="Center">
    <Styles>
      <Style Name="Default" FontColor="#F4E1C4FF" TextAlphaFactor="0.4" TextGlowColor="#000000FF" TextOutlineColor="#000000FF" TextOutlineAmount="0" TextGlowRadius="0" TextBlur="0" FontSize="28" />
      <Style Name="Hovered" FontColor="#F4E1C4FF" TextAlphaFactor="0.6" TextGlowColor="#000000FF" TextOutlineColor="#000000FF" TextOutlineAmount="0" TextGlowRadius="0" TextBlur="0" FontSize="28" />
      <Style Name="Pressed" FontColor="#F4E1C4FF" TextAlphaFactor="0.2" TextGlowColor="#000000FF" TextOutlineColor="#000000FF" TextOutlineAmount="0" TextGlowRadius="0" TextBlur="0" FontSize="28" />
      <Style Name="Selected" FontColor="#F4E1C4FF" TextAlphaFactor="0" TextGlowColor="#000000FF" TextOutlineColor="#000000FF" TextOutlineAmount="0" TextGlowRadius="0" TextBlur="0" FontSize="28" />
    </Styles>
  </Brush>
  <Brush Name="SaveLoad.DeleteSave.Button">
    <Layers>
      <BrushLayer Name="Default" Sprite="General\MenuElements\delete_icon" />
    </Layers>
    <Styles>
      <Style Name="Default">
        <BrushLayer Name="Default" ColorFactor="1" />
      </Style>
      <Style Name="Hovered">
        <BrushLayer Name="Default" ColorFactor="1.2" />
      </Style>
      <Style Name="Pressed">
        <BrushLayer Name="Default" ColorFactor="0.8" />
      </Style>
      <Style Name="Selected">
        <BrushLayer Name="Default" />
      </Style>
      <Style Name="Disabled">
        <BrushLayer Name="Default" AlphaFactor="0.6" SaturationFactor="-80" ValueFactor="-20" />
      </Style>
    </Styles>
  </Brush>
  
    <Brush Name="SaveLoad.Collapser">
    <Layers>
      <BrushLayer Name="Default" Sprite="SaveLoad\load_collapser" Color="#FFFFFFFF" ExtendBottom="8" ExtendTop="3" SaturationFactor="20" HueFactor="-10"/>
    </Layers>
    <Styles>
      <Style Name="Default">
        <BrushLayer Name="Default"  />
      </Style>
      <Style Name="Hovered">
        <BrushLayer Name="Default" ColorFactor="1.2" />
      </Style>
      <Style Name="Selected">
        <BrushLayer Name="Default" ColorFactor="1.5" Color="#FFC349FF" />
      </Style>
      <Style Name="Pressed">
        <BrushLayer Name="Default" ColorFactor="0.9" />
      </Style>
    </Styles>
    <SoundProperties>
      <StateSounds>
      </StateSounds>
      <EventSounds>
        <EventSound EventName="Click" Audio="filter" />
      </EventSounds>
    </SoundProperties>
  </Brush>
  
  <Brush Name="SaveLoad.Tuple.Title.Text" Font="Galahad" TextHorizontalAlignment="Left" TextVerticalAlignment="Center">
    <Styles>
      <Style Name="Default" FontColor="#EFC083CC" TextGlowColor="#000000FF" TextOutlineColor="#000000FF" TextOutlineAmount="0" TextGlowRadius="0" TextBlur="0" FontSize="36" />
