  <Brush Name="Saveload.Scrollbar.Handle">
    <Layers>
      <BrushLayer Name="Default" Sprite="BlankWhiteSquare_9" Color="#573524FF" />
    </Layers>
    <Styles>
      <Style Name="Default">
        <BrushLayer Name="Default" ColorFactor="1" AlphaFactor="0.9" />
