<Brushes>
  <Brush Name="Map.Notification.Sound">
    <Layers>
      <BrushLayer Name="Default" IsVisible="false" />
    </Layers>
    <SoundProperties>
      <EventSounds>
        <EventSound EventName="OnInspection" Audio="default" />
        <EventSound EventName="OnRemoveBegin" Audio="default" />
      </EventSounds>
    </SoundProperties>
  </Brush>
  <Brush Name="Map.Notification.Ring">
    <Layers>
      <BrushLayer Name="Default" Sprite="SPGeneral\MapNotification\notification_ring" />
    </Layers>
    <Styles>
      <Style Name="Default">
        <BrushLayer Name="Default" AlphaFactor="1.0" />
      </Style>
      <Style Name="Hovered">
        <BrushLayer Name="Default" AlphaFactor="1.0" />
      </Style>
      <Style Name="Pressed">
        <BrushLayer Name="Default" AlphaFactor="1.0" />
      </Style>
      <Style Name="Disabled">
        <BrushLayer Name="Default" AlphaFactor="1" />
      </Style>
    </Styles>
  </Brush>
  <Brush Name="Map.Notification.More.Panel">
    <Layers>
      <BrushLayer Name="Default" Sprite="SPGeneral\Tutorial\shadow" />
    </Layers>
    <Styles>
      <Style Name="Default" AnimationToPlayOnBegin="DefaultAnimation" HueFactor="-100" />
    </Styles>
    <Animations>
      <Animation Name="DefaultAnimation" Duration="1.5" Loop="true">
        <AnimationProperty LayerName="Default" PropertyName="ValueFactor">
          <KeyFrame Time="0.0" Value="0" />
          <KeyFrame Time="0.2" Value="5" />
          <KeyFrame Time="0.4" Value="8" />
          <KeyFrame Time="0.75" Value="30" />
          <KeyFrame Time="1.1" Value="8" />
          <KeyFrame Time="1.3" Value="5" />
          <KeyFrame Time="1.5" Value="0" />
        </AnimationProperty>
        <AnimationProperty LayerName="Default" PropertyName="SaturationFactor">
          <KeyFrame Time="0.0" Value="0" />
          <KeyFrame Time="0.2" Value="50" />
          <KeyFrame Time="0.4" Value="100" />
          <KeyFrame Time="0.75" Value="100" />
          <KeyFrame Time="1.1" Value="100" />
          <KeyFrame Time="1.3" Value="50" />
          <KeyFrame Time="1.5" Value="0" />
        </AnimationProperty>
      </Animation>
    </Animations>
  </Brush>
  <Brush Name="Map.Notification.Remove.Button">
    <Layers>
      <BrushLayer Name="Default" Sprite="MapMenuUnread@2x" Color="#381704FF" />
