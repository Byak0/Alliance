  <Brush Name="Tournament.Participant">
    <Layers>
      <BrushLayer Name="Default" Sprite="SPGeneral\SPScoreboard\party_bar_collapser" />
    </Layers>
    <Styles>
      <Style Name="Default">
        <BrushLayer Name="Default" ColorFactor="1" AlphaFactor="1" />
      </Style>
      <Style Name="Current" AnimationToPlayOnBegin="CurrentMatchAnimation">
        <BrushLayer Name="Default" ColorFactor="1" AlphaFactor="1" />
      </Style>
      <Style Name="Over">
        <BrushLayer Name="Default" ColorFactor="1" AlphaFactor="0.5" />
      </Style>
      <Style Name="Dead">
        <BrushLayer Name="Default" ColorFactor="1" AlphaFactor="1" />
      </Style>
    </Styles>
    <Animations>
      <Animation Name="CurrentMatchAnimation" Duration="1" Loop="true">
        <AnimationProperty PropertyName="ColorFactor" LayerName="Default">
          <KeyFrame Time="0.0" Value="1" />
          <KeyFrame Time="0.5" Value="2.5" />
          <KeyFrame Time="1" Value="1" />
        </AnimationProperty>
      </Animation>
    </Animations>
  </Brush>
