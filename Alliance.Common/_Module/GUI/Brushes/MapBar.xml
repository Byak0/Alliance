  <Brush Name="MapTextBrushWithAnim" Font="Galahad" TextHorizontalAlignment="Left">
    <Layers>
      <BrushLayer Name="Default" Color="#FAF4DEFF" />
    </Layers>
    <Styles>
      <Style Name="Default" FontColor="#F4E1C4FF" TextGlowColor="#000000FF" TextOutlineColor="#FFFFFF00" TextOutlineAmount="0" TextGlowRadius="0" TextBlur="0" AnimationToPlayOnBegin="ValueChangeAnimation" FontSize="21" />
      <Style Name="Negative" AnimationToPlayOnBegin="NegativeValueChangeAnimation" />
      <Style Name="Positive" AnimationToPlayOnBegin="PositiveValueChangeAnimation" />
    </Styles>
    <Animations>
      <Animation Name="PositiveValueChangeAnimation" Duration="0.5" Loop="false">
        <AnimationProperty PropertyName="FontColor">
          <KeyFrame Time="0.0" Value="#F4E1C4FF" />
          <KeyFrame Time="0.01" Value="#A8D521FF" />
          <KeyFrame Time="0.02" Value="#A8D521FF" />
          <KeyFrame Time="0.04" Value="#A8D521FF" />
          <KeyFrame Time="0.05" Value="#A8D521FF" />
          <KeyFrame Time="0.6" Value="#A8D521FF" />
        </AnimationProperty>
        <AnimationProperty PropertyName="TextGlowRadius">
          <KeyFrame Time="0.0" Value="0" />
          <KeyFrame Time="0.01" Value="0.0" />
        </AnimationProperty>
      </Animation>
      <Animation Name="NegativeValueChangeAnimation" Duration="0.6" Loop="false">
        <AnimationProperty PropertyName="FontColor">
          <KeyFrame Time="0.0" Value="#F4E1C4FF" />
          <KeyFrame Time="0.01" Value="#FF0000FF" />
          <KeyFrame Time="0.02" Value="#FF0000FF" />
          <KeyFrame Time="0.04" Value="#FF0000FF" />
          <KeyFrame Time="0.05" Value="#FF0000FF" />
          <KeyFrame Time="0.6" Value="#FF0000FF" />
        </AnimationProperty>
      </Animation>
    </Animations>
  </Brush>

